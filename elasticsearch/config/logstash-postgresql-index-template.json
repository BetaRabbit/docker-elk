{
  "template": "logstash-postgresql-*",
  "order": 1,
  "mappings": {
    "postgresql": {
      "properties": {
        "host": {
          "type": "string",
          "index": "not_analyzed"
        },
        "pid": {
          "type": "integer"
        },
        "session_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "user_id": {
          "type": "string",
          "index": "not_analyzed"
        },
        "db_name": {
          "type": "string",
          "index": "not_analyzed"
        },
        "app_name": {
          "type": "string",
          "index": "not_analyzed"
        },
        "client": {
          "type": "string",
          "index": "not_analyzed"
        },
        "duration": {
          "type": "float"
        },
        "statement": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    }
  }
}